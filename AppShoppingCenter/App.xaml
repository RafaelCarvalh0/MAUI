                <ResourceDictionary Source="Resources/Styles/ExplicityStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <!--<converter:TextChangedEventArgsConverter x:Key="TextChangedEventArgsConverter" />-->
        </ResourceDictionary>
    </Application.Resources>
</Application>

